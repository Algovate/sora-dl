{
  "name": "sora-dl",
  "version": "1.2.1",
  "description": "A powerful Sora video downloader with metadata extraction",
  "main": "dist/index.js",
  "bin": {
    "sora-dl": "dist/cli.js"
  },
  "scripts": {
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:watch": "webpack --mode development --watch",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "install-global": "npm install -g .",
    "test": "node dist/cli.js download local examples/feed.json --count 1 --list",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts --max-warnings 0",
    "prepare": "npm run build"
  },
  "keywords": [
    "video",
    "downloader",
    "sora",
    "metadata",
    "cli",
    "openai",
    "feed"
  ],
  "author": "Algovate Lab",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/algovate/sora-dl.git"
  },
  "homepage": "https://github.com/algovate/sora-dl#readme",
  "bugs": {
    "url": "https://github.com/algovate/sora-dl/issues"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "commander": "^14.0.1",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^22.0.0",
    "@types/progress": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ]
}
